    diabox_tween_duration = 60, //Duration of tween for diabox
    line_font = ft_arial_18, //Font for the dialogue
    line_color = c_white, //Color for dialogue text
    line_max_length = 950, //Max length of string in a line before break
    line_speed_letter = 1.25, //Speed of each letter appearing
    line_xoffset = 25, //X Offset of the line as margin on box
    line_yoffset = 20, //Y Offset of the line as margin on box
    
    //Name Widget
    name_draw_box = true, //Boolean if the box for the name should be drawn.
    name_color = c_white, //Color for name text
    name_font = ft_arial_20_bold, //Font for name
    name_field_xoffset = 512, //X offset for namefield
    name_field_yoffset = -35, //Y offset for namefield
    name_actor_xoffset = 512, //X offset for actor name
    name_actor_yoffset = 50, //Y offset for actor name
    
    //Choice Widget
    choice_font = ft_arial_18, //Font for the choices
    choices_max = 4, //Maximum number of choices
    choice_color_on = c_gray, //Color for highlighted choice
    choice_color_off = c_white, //Default color for choices
    
    //Actors
    actors_max = 3, //Maximum number of actors. If increased, goto "scr_dialogue_object" to extend location prefixed positions.
    actor_left_xoffset = 0, //X Offset of the Left Actor (based on Dialogue Widget)
    actor_left_yoffset = 100, //Y Offset of the Left Actor (based on Dialogue Widget)
    actor_right_xoffset = 600, //X Offset of the Right Actor (based on Dialogue Widget)
    actor_right_yoffset = 100, //Y Offset of the Right Actor (based on Dialogue Widget)
    actor_middle_xoffset = 300, //X Offset of the Middle Actor (based on Dialogue Widget)
    actor_middle_yoffset = 100 //Y Offset of the Middle Actor (based on Dialogue Widget)
}

